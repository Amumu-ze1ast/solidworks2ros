  <!-- Xacro Macro to define a rope link and joint -->
  <xacro:macro name="rope_joint" params="index">
    <!-- Define the link -->
    <link name="link_${index}">
      <inertial>
        <origin 
          xyz="-1.50990331349021E-14 -1.79856129989275E-14 -0.254000177901664" 
          rpy="0 0 0" />
        <mass 
