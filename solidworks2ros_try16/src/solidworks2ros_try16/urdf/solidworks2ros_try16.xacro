        </material>
      </visual>
      <collision>
        <origin 
          xyz="0 0 0" 
          rpy="0 0 0" />
        <geometry>
          <mesh filename="package://solidworks2ros_try16/meshes/link_4.STL" />
        </geometry>
      </collision>
    </link>

    <!-- Define the joint -->
    <joint name="joint_${index}" 
      type="prismatic">
      <origin 
        xyz="0 0.1143 -0.0762" 
        rpy="0 0 0" />
      <parent 
        link="link_2" />
      <child 
        link="link_${index}" />
      <axis 
        xyz="0 0 -1" />
      <limit 
        lower="0" 
        upper="20" 
        effort="100" 
        velocity="0.2" />
    </joint>
  </xacro:macro>

