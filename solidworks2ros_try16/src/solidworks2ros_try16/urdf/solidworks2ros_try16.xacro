          ixy="8.63968603247427E-09"
          ixz="5.94410662360116E-18"
          iyy="0.0123412022009673"
          iyz="2.06563437257844E-18"
          izz="0.000129968031935081" />
      </inertial>
      <visual>
        <origin 
          xyz="0 0 0" 
          rpy="0 0 0" />
        <geometry>
          <mesh filename="package://solidworks2ros_try16/meshes/link_4.STL" />
        </geometry>
        <material name="">
