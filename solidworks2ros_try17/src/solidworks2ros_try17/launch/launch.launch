<?xml version="1.0"?>
<launch>

    <param name="robot_description" textfile="$(find solidworks2ros_try17)/urdf/solidworks2ros_try17.urdf"/> 
    
    <include file="$(find gazebo_ros)/launch/empty_world.launch" />
    
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -model solidworks2ros_try17"    />

    <node name="Robot_State_Publisher" pkg="robot_state_publisher" type="robot_state_publisher" output="screen"/>

    <rosparam  file="$(find solidworks2ros_try17)/config/position_controller.yaml"/>

    <node name="Controller_Spawner" pkg="controller_manager" type="spawner" args="joint_1_controller joint_2_controller joint_3_controller joint_4_controller" />

    

</launch>